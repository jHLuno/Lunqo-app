{
  "name": "lunqo-app",
  "version": "1.0.0",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop lunqo-app",
    "pm2:restart": "pm2 restart lunqo-app",
    "pm2:logs": "pm2 logs lunqo-app",
    "pm2:status": "pm2 status",
    "deploy": "bash deploy.sh",
    "build": "webpack --mode production",
    "dev:client": "webpack serve --mode development",
    "postinstall": "echo 'Puppeteer installation complete. Chrome will be downloaded on first use.'",
    "security:audit": "node security-audit.js",
    "security:migrate-passwords": "node migrate-passwords.js",
    "security:generate-secret": "node -e \"console.log(require('crypto').randomBytes(64).toString('hex'))\"",
    "security:check": "npm run security:audit && npm run security:migrate-passwords"
  },
  "keywords": [
    "adtech",
    "taxi",
    "digital-ads",
    "smartseat-media"
  ],
  "author": "Lunqo Team",
  "license": "ISC",
  "description": "Unified control, real-time insights, effortless scaling for in-taxi media",
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "framer-motion": "^10.16.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.292.0",
    "mongoose": "^8.0.0",
    "react": "^18.2.0",
    "react-countup": "^6.4.2",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.2",
    "react-router-dom": "^6.20.1"
  },
  "optionalDependencies": {
    "puppeteer": "^24.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-runtime": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/runtime": "^7.23.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^13.0.0",
    "core-js": "^3.33.0",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^30.1.3",
    "jest-environment-node": "^30.1.2",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "style-loader": "^3.3.3",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.3.5",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  }
}
